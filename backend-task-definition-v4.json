{
  "family": "cows-shelter-app",
  "executionRoleArn": "arn:aws:iam::002824154008:role/cows-shelter-ecs-task-execution-role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "002824154008.dkr.ecr.eu-north-1.amazonaws.com/cows-shelter-app:20250701155106",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "cows-shelter-db.cfcuqq86u682.eu-north-1.rds.amazonaws.com"
        },
        {
          "name": "DB_PORT",
          "value": "5432"
        },
        {
          "name": "DB_USER",
          "value": "postgres"
        },
        {
          "name": "DB_PASSWORD",
          "value": "OsIUxvObUVCUtIiW"
        },
        {
          "name": "DB_NAME",
          "value": "cows_shelter"
        },
        {
          "name": "GIN_MODE",
          "value": "release"
        },
        {
          "name": "MINIO_ENDPOINT",
          "value": "s3.eu-north-1.amazonaws.com"
        },
        {
          "name": "MINIO_ACCESS_KEY",
          "value": "minioadmin"
        },
        {
          "name": "MINIO_SECRET_KEY",
          "value": "minioadmin"
        },
        {
          "name": "MINIO_USE_SSL",
          "value": "true"
        },
        {
          "name": "MINIO_BUCKET",
          "value": "cows-shelter"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/cows-shelter-app",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048"
}
